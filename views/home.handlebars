{{!-- Preset posts on screen --}}
{{#each posts as |post|}}
<div class="post-card">
    <div>
        <h2>
            {{post.title}}
        </h2>
        <p>
            {{post.date}} by {{user.user_name}}
        </p>
        <p>
            {{post.description}}
        </p>
    </div>
</div>
{{/each}}